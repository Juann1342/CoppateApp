<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/ma_lv_lista_eventos"
    android:orientation="horizontal">


    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/icono64"
        android:id="@+id/img_lista_eventos_otros" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/txt_lista_eventos_otros"
        android:layout_alignBottom="@+id/img_lista_eventos_otros"
        android:layout_toRightOf="@+id/img_lista_eventos_otros"
        android:layout_toEndOf="@+id/img_lista_eventos_otros"
        android:layout_marginLeft="12dp"
        android:layout_marginStart="12dp"
        android:layout_marginBottom="19dp" />


</RelativeLayout>
