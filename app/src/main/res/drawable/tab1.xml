<?xml version="1.0" encoding="utf-8"?>
<!--en este archivo cambiamos el color del icono de la tab1 segun si esta seleccionado o no-->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!--muestra el icono cuando esta seleccionado-->
    <item android:drawable="@drawable/miseventos1"
        android:state_selected="true"/>
    <!--muestra el icono cuando  no esta seleccionado-->
    <item android:drawable="@drawable/miseventos"
        android:state_selected="false"/>
</selector>